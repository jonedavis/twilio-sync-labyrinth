{
  "name": "twilio",
  "description": "A Twilio helper library",
  "version": "3.0.0-rc.7",
  "author": "API Team <api@twilio.com>",
  "contributors": [
    {
      "name": "twilio-node contributors",
      "url": "https://github.com/twilio/twilio-node/graphs/contributors"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio/twilio-node.git"
  },
  "dependencies": {
    "deprecate": "^0.1.0",
    "jsonwebtoken": "5.4.x",
    "jwt-simple": "0.1.x",
    "lodash": "4.0.0",
    "moment": "2.11.1",
    "q": "2.0.x",
    "request": "2.67.x",
    "rootpath": "0.1.2",
    "scmp": "0.0.3"
  },
  "devDependencies": {
    "express": "3.x",
    "jasmine-node": "1.14.5",
    "jshint": "2.8.x",
    "jscs": "2.6.x",
    "jsdoc": "3.4.x"
  },
  "scripts": {
    "test": "jasmine-node --captureExceptions spec",
    "jshint": "jshint lib/rest/** lib/base/** lib/http/** spec/integration/**",
    "jscs": "jscs -c .jscs lib/rest/** lib/base/** lib/http/**",
    "check": "npm run jshint; npm run jscs",
    "jsdoc": "jsdoc -r lib -d docs"
